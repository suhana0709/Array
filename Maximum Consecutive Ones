#program to find the maximum consecutive 1's
def maximum_ones(a, a_size):
    counter = 0
    max_ones = 0
    for i in range(0, a_size):
        if (a[i] == 0):
            counter = 0
        else:
            counter += 1
            max_ones = max(counter, max_ones)
    return max_ones

a = [0,1,1,1,0,0,1,0,1,1,1,1,1]
print("Array: ",a)
print("Maximum Consecutive Ones = ",maximum_ones(a, len(a)))
